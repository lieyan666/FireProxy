{
  "api": {
    "enabled": true,
    "host": "127.0.0.1",
    "port": 8080,
    "enableCors": true
  },
  "logging": {
    "level": "info",
    "enableFile": true,
    "enableConsole": true,
    "logDir": "./logs",
    "maxFileSize": 10485760,
    "maxFiles": 5
  },
  "forward": [
    {
      "id": 1,
      "name": "Existing TCP Rule",
      "type": "tcp",
      "status": "active",
      "localHost": "0.0.0.0",
      "localPort": 3333,
      "targetHost": "192.168.1.123",
      "targetPort": 1234
    },
    {
      "id": 2,
      "name": "Existing UDP Rule",
      "type": "udp",
      "status": "active",
      "localHost": "192.168.1.3",
      "localPort": 3456,
      "targetHost": "192.168.1.10",
      "targetPort": 5678
    },
    {
      "id": 3,
      "name": "TCP Port Range Example",
      "type": "tcp",
      "status": "active",
      "localHost": "0.0.0.0",
      "localPortRange": [4000, 4010],
      "targetHost": "192.168.1.124",
      "targetPortRange": [5000, 5010]
    },
    {
      "id": 4,
      "name": "UDP Port Range Example",
      "type": "udp",
      "status": "active",
      "localHost": "192.168.1.4",
      "localPortRange": [6000, 6005],
      "targetHost": "192.168.1.11",
      "targetPortRange": [7000, 7005]
    }
  ]
}
